generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model User {
  id         String   @id
  first_name String
  last_name  String?
  image      String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
